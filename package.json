{
  "name": "aframe-periodictable",
  "version": "0.0.2",
  "description": "A-Frame Sample",
  "author": "Norihito Nishioka (https://github.com/nishioka)",
  "main": "src/js/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nishioka/aframe-periodictable.git"
  },
  "keywords": [
    "aframe",
    "webpack"
  ],
  "scripts": {
    "babel": "babel-node --presets es2015",
    "clean": "npm-run-all clean:*",
    "clean:dist": "rimraf dist/* !dist/.gitignore",
    "clean:tmp": "rimraf tmp/* !tmp/.gitignore",
    "clean:coverage": "rimraf devel/coverage/* !devel/coverage/.gitignore",
    "clean:npm": "rimraf npm-debug.log",
    "precoverage": "npm run vendor:perf",
    "coverage": "npm-run-all --parallel coverage:run --serial coverage:check",
    "coverage:run": "babel-node ./node_modules/.bin/istanbul cover --root ./src --config ./config/istanbul.yaml npm run test:ci",
    "coverage:check": "istanbul check-coverage --root ./devel/coverage",
    "lint": "npm-run-all lint:*",
    "lint:stylelint": "stylelint \"./src/sass/**/*.scss\" --syntax scss --config ./config/stylelint.json",
    "lint:semistandard": "eslint --no-eslintrc --ignore-path ./config/eslintignore.txt --config ./config/eslint.json ./src",
    "pretest": "npm run clean:coverage",
    "test": "set NODE_ENV=test && babel-node ./node_modules/karma/bin/karma start ./config/karma.conf.babel.js",
    "test:ci": "npm run test -- --single-run --browsers Firefox",
    "test:watch": "npm run test -- --watch",
    "test:grep": "npm run test -- --watch --grep $pattern",
    "precommit": "npm run verify",
    "verify": "npm-run-all lint test:ci",
    "vendor:perf": "webpack --config ./config/webpack-perf.js",
    "prebuild": "npm-run-all clean:dist vendor:perf",
    "build": "webpack --config ./config/webpack.config.babel.js --env.devel --progress",
    "start": "set NODE_ENV=test && npm-run-all build dev",
    "deploy": "npm run build && gh-pages -d dist",
    "dev": "babel-node devel --env.devel --hot --port 9000"
  },
  "dependencies": {
    "aframe": "^0.7.1",
    "aframe-extras": "^3.12.4",
    "babel-polyfill": "^6.26.0",
    "cannon": "^0.6.2",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "chai": "^4.1.2",
    "chai-shallow-deep-equal": "^1.4.6",
    "chalk": "^2.1.0",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.8.0",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "exports-loader": "^0.6.4",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.0.0",
    "handlebars-loader": "^1.4.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "imports-loader": "^0.7.1",
    "ip": "^1.1.5",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chai-shallow-deep-equal": "^0.0.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "shebang-command": "^1.2.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "ignore": [
      "config/**",
      "devel/**",
      "dist/**",
      "tmp/**",
      "**/vendors/**"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "env": {
      "test": {},
      "development": {},
      "production": {}
    }
  }
}
